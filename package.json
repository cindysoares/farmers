{
  "name": "farmers",
  "version": "1.0.0",
  "scripts": {
    "dropdb": "npx sequelize db:drop",
    "createdb": "npx sequelize db:create",
    "createschema": "npx sequelize db:migrate",
    "dummydata": "npx sequelize db:seed:all",
    "express": "node server.js",
    "start-server": "npm-run-all build express",
    "ng": "ng",
    "start": "npm-run-all -c dropdb createdb createschema dummydata start-server",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ]
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "body-parser": "^1.19.0",
    "core-js": "^2.5.4",
    "express": "^4.17.1",
    "pg": "^8.2.1",
    "rxjs": "~6.3.3",
    "sequelize": "^5.21.12",
    "sequelize-auto": "^0.4.29",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.0.4",
    "@angular/cli": "~7.3.10",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^25.5.4",
    "jest-preset-angular": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
